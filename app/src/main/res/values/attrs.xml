<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ConnectingRipple">
        <attr name="ripple_color" format="color" />
        <attr name="enable_single_ripple" format="boolean" />
        <attr name="ripple_duration" format="integer" />
        <attr name="enable_color_transition" format="boolean" />
        <attr name="enable_random_position" format="boolean" />
        <attr name="enable_random_color" format="boolean" />
        <attr name="enable_stroke_style" format="boolean" />
        <attr name="ripple_from_color" format="color" />
        <attr name="ripple_to_color" format="color" />
        <attr name="ripple_stroke_width" format="dimension" />
        <attr name="ripple_maximum_radius" format="dimension" />
        <attr name="ripple_count" format="integer" />
    </declare-styleable>

    <declare-styleable name="Switcher">
        <attr name="elevation" />
        <attr name="switcher_width" format="dimension"/>
        <attr name="switcher_height" format="dimension"/>
        <attr name="android:checked" format="boolean" />
        <attr name="switcher_on_color" format="color" />
        <attr name="switcher_off_color" format="color" />
        <attr name="switcher_icon_color" format="color" />
    </declare-styleable>
</resources>